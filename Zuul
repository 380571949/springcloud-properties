#zuul01

server.port=8103
zuul.sensitive-headers=
#service name
spring.application.name=service-zuul

eureka.instance.preferIpAddress=true
eureka.client.registerWithEureka=true
eureka.client.fetchRegistry=true
eureka.client.serviceUrl.defaultZone=http://localhost:8888/eureka/
#service-hi
zuul.routes.api-a.path=/zuul-hi/**
zuul.routes.api-a.serviceId=service-hi
#service-ribbon
zuul.routes.api-b.path=/zuul-ribbon/**
zuul.routes.api-b.serviceId=service-ribbon
#ordertest
zuul.routes.ordertest.path=/zuul-order-test/**
zuul.routes.ordertest.serviceId=ordertest
#usertest
zuul.routes.usertest.path=/zuul-user-test/**
zuul.routes.usertest.serviceId=usertest
#product-service
zuul.routes.product.path=/zuul-product/**
zuul.routes.product.serviceId=product-service
#pic-service
zuul.routes.pic.path=/zuul-pic/**
zuul.routes.pic.serviceId=pic-service
#user-service
zuul.routes.user.path=/zuul-user/**
zuul.routes.user.serviceId=user-service
#cart-service
zuul.routes.cart.path=/zuul-cart/**
zuul.routes.cart.serviceId=cart-service
#order-service
zuul.routes.order.path=/zuul-order/**
zuul.routes.order.serviceId=order-service
#search-service
zuul.routes.search.path=/zuul-search/**
zuul.routes.search.serviceId=search-service
#seckill-service
zuul.routes.seckill.path=/zuul-seckill/**
zuul.routes.seckill.serviceId=seckill-service
...





